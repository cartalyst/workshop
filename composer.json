{
    "name": "cartalyst/workshop",
    "description": "Generators for scaffolding Cartalyst Platform resources",
    "keywords": [
        "platform",
        "extensions",
        "laravel",
        "bootstrap"
    ],
    "license": "proprietary",
    "authors": [
        {
            "name": "Cartalyst LLC",
            "email": "help@cartalyst.com",
            "homepage": "https://cartalyst.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^5.0"
    },
    "require-dev": {
        "cartalyst/php-cs-fixer-config": "^1.0",
        "illuminate/filesystem": "^5.0",
        "laravelcollective/html": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Cartalyst\\Workshop\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cartalyst\\Workshop\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "component": "package",
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
