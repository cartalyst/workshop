<script type="text/template" data-grid="{{grid_name}}" data-grid-template="results">

    <% var results = response.results; %>

    <% if (_.isEmpty(results)) { %>

        <tr>
            <td colspan="5">{{{ trans('common.no_results') }}}</td>
        </tr>

    <% } else { %>

        <% _.each(results, function(r) { %>

            <tr data-grid-row>
                {{columns}}
            </tr>

        <% }); %>

    <% } %>

</script>
