'register' => function(ExtensionInterface $extension, Application $app)
	{
		${{lower_name}}Repository = '{{vendor}}\{{name}}\Repositories\{{model}}RepositoryInterface';

		if ( ! $app->bound(${{lower_name}}Repository))
		{
			$app->bind(${{lower_name}}Repository, function($app)
			{
				$model = get_class($app['{{vendor}}\{{name}}\Models\{{model}}']);

				return new {{vendor}}\{{name}}\Repositories\Db{{model}}Repository($model, $app['events']);
			});
		}
	},
